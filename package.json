{
  "name": "job-back-end-course",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:Viktor-sun/job-back-end-course.git",
  "author": "Viktor-sun <bobick.zdoh@gmail.com>",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "mongoose": "^7.5.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/node": "^20.7.0",
    "eslint": "^8.52.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "start": "yarn build && node ./dist/server.js",
    "dev": "nodemon ./src/server.ts",
    "dev:old": "nodemon --exec 'node --no-warnings=ExperimentalWarning --loader ts-node/esm' src/server.ts",
    "prebuild": "rimraf ./dist",
    "build": "tsc --project ./",
    "typeorm": "npx typeorm-ts-node-esm -d src/model/postgresql/dataSource.ts",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert"
  }
}
